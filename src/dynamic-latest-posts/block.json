{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "my-theme/dynamic-latest-posts",
	"version": "0.1.0",
	"title": "Latest Posts",
	"category": "widgets",
	"icon": "smiley",
	"description": "Latest Posts.",
	"example": {},
	"attributes": {
		"Maximum": {
			"type": "integer",
			"default": 3
		},
		"showFeaturedImage": {
			"type": "boolean"
		},		
		"showDescription": {
			"type": "boolean"
		},
		"showDate": {
			"type": "boolean"
		}
	},
	"supports": {
		"color": {
			"background": true,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "my-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}